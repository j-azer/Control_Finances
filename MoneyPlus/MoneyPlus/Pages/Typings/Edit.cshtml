@page
@model MoneyPlus.Pages.Typings.EditModel

@{
    ViewData["Title"] = "Edit Typings";
}

<h1 class="display-5">@ViewData["Title"]</h1>
<br/>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Typing.Id" />
            <div class="form-group">
                <label asp-for="Typing.Type" class="control-label"></label>
                <select asp-for="Typing.Type" class="form-control"></select>
                <span asp-validation-for="Typing.Type" class="text-danger"></span>
            </div>
            <br/>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" /> | <a class="btn btn-secondary" asp-page="./Index">Back to List</a>
            </div>
        </form>
    </div>
</div>

<div>
    
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
